---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>the negation</title>
  <link href="http://thenegation.com/atom.xml" rel="self"/>
  <link href="http://thenegation.com"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://thenegation.com/</id>
  <author>
    <name>Vehbi Sinan Tunalioglu</name>
    <email>vst@vsthost.com</email>
  </author>
  {% for post in site.posts limit: 20 %}
  <entry>
    <title>{{ post.title }}</title>
    <link href="http://thenegation.com/{{ post.url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>http://thenegation.com/{{ post.id }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
</feed>
